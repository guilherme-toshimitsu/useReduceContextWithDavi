{
  "name": "contextapi-davi-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "dev": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "test:cov": "react-app-rewired test --coverage --watchAll=false && npx serve -l 5000 ./coverage/lcov-report",
    "test:update": "react-app-rewired test --coverage --watchAll=false",
    "eslint": "node ./node_modules/eslint/bin/eslint.js src --ext .js,.jsx",
    "prettier": "prettier --write {src,server,jest,stories,__tests__,storybook}/**/*.js",
    "precommit": "NODE_ENV=production lint-staged",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook"
  },
  "lint-staged": {
    "*.{js,json,scss,css,html}": [
      "npm run prettier",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.7.2",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/styles": "^4.7.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "axios": "^0.19.0",
    "clsx": "^1.0.4",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@storybook/react": "^5.2.8",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-preset-es2015": "^6.24.1",
    "copy-webpack-plugin": "^5.1.0",
    "customize-cra": "^0.9.1",
    "enzyme": "^3.10.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-watch": "^6.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.1",
    "pretty-quick": "^2.0.1",
    "react-app-rewired": "^2.1.5",
    "react-hot-loader": "^4.12.18",
    "react-scripts-ts": "^3.1.0",
    "sass-loader": "^8.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "src/serviceWorker.js",
      "src/services/api/base.js",
      "src/setupTests.js",
      "src/index.js",
      "src/App.js",
      "src/routes/",
      "src/commons/theme.js",
      "src/services/mock"
    ],
    "moduleNameMapper": {
      "@api": "<rootDir>/src/services/api/",
      "@assets/(.*)$": "<rootDir>/src/assets/$1",
      "@commons/(.*)$": "<rootDir>/src/commons/$1",
      "@components/(.*)$": "<rootDir>/src/components/$1",
      "@config": "<rootDir>/src/config/",
      "@fonts/(.*)$": "<rootDir>/src/assets/fonts/$1",
      "@images/(.*)$": "<rootDir>/src/assets/images/$1",
      "@pages/(.*)$": "<rootDir>/src/pages/$1",
      "@routes": "<rootDir>/src/config/routes/",
      "@services/(.*)$": "<rootDir>/src/services/$1",
      "@utils/(.*)$": "<rootDir>/src/commons/utils/$1"
    }
  }
}
